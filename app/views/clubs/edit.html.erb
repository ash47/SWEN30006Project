<h1>Editing <strong><%= @club.name %></strong></h1>

<%= form_for(@club) do |f| %>
  <%= render template: 'clubs/form_error' %>

  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :website %><br>
    <%= f.text_field :website %>
  </div>
  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>

<%= link_to 'Show', @club %> |
<%= link_to 'Back', clubs_path %>
